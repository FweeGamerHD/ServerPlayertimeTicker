{
  "schemaVersion": 1,
  "id": "serverplayertimeticker",
  "version": "${version}",
  "name": "ServerPlayertimeTicker",
  "description": "ServerPlayertimeTicker is a Fabric mod that manages player playtime on Minecraft servers, providing administrators with tools to enforce fair gameplay limits.",
  "authors": ["FreeGamer"],
  "contact": {
    "repo": "https://github.com/freeg/ServerPlayertimeTicker"
  },
  "license": "CC-BY-NC-SA-4.0",
  "environment": "*",
  "entrypoints": {
    "main": [
      "freegamerdev.serverplayertimeticker.ServerPlayertimeTicker"
    ]
  },
  "mixins": [
    "serverplayertimeticker.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "luckperms": "*"
  }
}
